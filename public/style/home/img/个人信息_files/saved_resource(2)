	$(function () {
		var console = window.console || { log: function () {} };
		$image = $('#image');
		var $dataX = $('#dataX');
		var $dataY = $('#dataY');
		var $dataHeight = $('#dataHeight');
		var $dataWidth = $('#dataWidth');
		var options = {
	        aspectRatio: 1 / 1,
	        preview: '.img-preview',
	        movable: false,
	        zoomable: false,
	        strict: false,
	        viewMode:1,
	        rotatable: false,
	        scalable: false,
	        crop: function (e) {
	          $dataX.val(Math.round(e.x));
	          $dataY.val(Math.round(e.y));
	          $dataHeight.val(Math.round(e.height));
	          $dataWidth.val(Math.round(e.width));
	        }
		};
		// Cropper
		$image.cropper(options);

		// Import image
		/* var $inputImage = $('#inputImage');
		var URL = window.URL || window.webkitURL;
		var blobURL;
		if (URL) {
			$inputImage.change(function () {
				var files = this.files;
				var file;
				if (!$image.data('cropper')) {
					return;
				}
				if (files && files.length) {
					file = files[0];
					if (/^image\/\w+$/.test(file.type)) {
						blobURL = URL.createObjectURL(file);
						$image.one('built.cropper', function () {
							URL.revokeObjectURL(blobURL);
						}).cropper('reset').cropper('replace', blobURL);
						changeEditHead("show");
					} else {
						showalert("请选择图片格式的文件");
						$inputImage.val("");
					}
				}
			});
		} else {
			$inputImage.change(function(){
				var f = $("#inputImage");
				if (this.value.match(/.jpg|.gif|.png|.bmp/i)) {
					subopt();
				} else {
					showalert("请选择图片格式的文件");
				}
			});
		} */
		
		$('#inputImage').change(function(){
			if (this.value.match(/.jpg|.png|.bmp/i)) {
				var f = fileChange(this);
				if(f){
					subfile();
				}
			} else {
				showalert("请选择JPG、PNG格式的文件");
			}
		});
		
		$('#cancelImg').on('click', function () {
			if($(".img-container").css("display")=='block'){
				showLayer(cancelEdit,"确定要取消编辑吗?");
			}else{
				showalert("您当前没有正在编辑的图片文件");
			}
		});
		
		//submit
		$('#uploadImg').on('click', function () {
			if($(".img-container").css("display")=='block'){
				subopt();
			}else{
				showalert("请选择您要更新的图片文件");
			}
		});
	});

	function changeEditHead(val){
		if("show"==val){
			$(".uphead_lt").hide();
			$(".img-container").show();
		}else if("hide"==val){
			$(".img-preview").html("");
			$(".img-preview").html("<div style='margin-top:40%'>没有预览</div>");
			$(".img-container").hide();
			$(".uphead_lt").show();
		}
	}

	//cancel edit
	function cancelEdit(){
		$image.on('built.cropper',function (){
		}).cropper('replace','/framework/common/images/tm.png').cropper('clear');
		$("#imgurl").val("");
		changeEditHead("hide");
	}

	//submit
	function subfile(){
		$("#imgForm").ajaxSubmit({
			url: '/info/uploadf.jhtml',
			type: 'post',
			dataType:"json",
			beforeSubmit:function(){
				if ($("#inputImage").val()==""){
					showalert("请选择要上传的图片");
	        		return false;
	        	}
				if (!$("#inputImage").val().match(/.jpg|.png|.bmp/i)) {
					showalert("请选择JPG、PNG格式的文件");
	        		return false;
				}
				//显示等待
				showWaitWindow("加载中，请稍等...");
	        }, 
			success:function(data){
				if(data.success){
					$("#imgurl").val(data.imgurl);
			        $image.cropper('replace','/info/getinput.jhtml?sts='+$("#sts").val()+'&url='+data.imgurl);
			        changeEditHead("show");
				}else{
					showfadeOutAssignSenc(data.msg,2);
				}
				hideWaitWindow();//关闭等待
	        },
	        error:function(XmlHttpRequest,textStatus,errorThrown){
	        	hideWaitWindow();//关闭等待
	        	showalert("所选图片过大");
	        }
		});
	    return false;
	}
	
	//update
	function subopt(){
		$("#coordForm").ajaxSubmit({
			url: '/info/changehead.jhtml',
			type: 'post',
			dataType:"json",
			beforeSubmit:function(){
				//显示等待
				showWaitWindow("头像更新中...");
	        }, 
			success:function(data){
				hideWaitWindow();//关闭等待
				if(data.success){
			        //changeEditHead("hide");
					cancelEdit();
			        showfadeOutAssignSenc("头像更新成功，请刷新查看",2);
				}else{
					showfadeOutAssignSenc(data.msg,2);
				}
	        },
	        error:function(XmlHttpRequest,textStatus,errorThrown){
	        	hideWaitWindow();//关闭等待
	        	showalert("头像更新失败");
	        }
		});
	    return false;
	}
	
	var ii = /msie/i.test(navigator.userAgent) && !window.opera;
	var i7 = /msie 7/i.test(navigator.userAgent);
	var i8 = /msie 8/i.test(navigator.userAgent);
	var i9 = /msie 9/i.test(navigator.userAgent);
	function fileChange(target) {
		if(i7 || i8 || i9){
			return true;
		}
		var fileSize = 0;
		var filemaxsize = 4;//1M 
		if (ii && !target.files) {
			var filePath = target.value;
			var fileSystem = new ActiveXObject("Scripting.FileSystemObject");
			if (!fileSystem.FileExists(filePath)) {
				showfadeOutAssignSenc("图片选择错误",2);
				return false;
			}
			var file = fileSystem.GetFile(filePath);
			fileSize = file.Size;
		} else {
			fileSize = target.files[0].size;
		}
		var size = fileSize/1024/1024;
		if (size > filemaxsize) {
			showfadeOutAssignSenc("文件大小超过4M，请重新选择",2);
			return false;
		}
		if (size <= 0) {
			showfadeOutAssignSenc("图片不能小于0M",2);
			return false;
		}
		return true;
	}
	
