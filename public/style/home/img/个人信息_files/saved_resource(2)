//初始化省
function initaddr(province,city,dist){
	// 省份数据遍历
	$.each(data1, function(k, p) {
		$("#province").append(_option(p.name, p.code,province));
		if(province){
			initcity(city);
			initdist(dist);
		}
	});
	// 省onchange事件
	$("#province").change(initcity);
	// 市onchange事件
	$("#city").change(initdist);
	$("#dist").change(function(){
		$("#_d").val($("#dist option:selected").text());
	});
}

//初始化城市
function initcity(ucity){
	var province=$("#province").val();
	_remove("city");
	_remove("dist");
	_each(data2, "#city", province,ucity);
	$("#_p").val($("#province option:selected").text());
}
//初始化县区
function initdist(udist){
	var city=$("#city").val();
	_remove("dist");
	_each(data3, "#dist", city,udist);
	$("#_c").val($("#city option:selected").text());
}

//生成select的option,n=name,v=value,un=用户的所在省
function _option(n, v ,un) {
	if(n==un){
		return "<option value='" + v + "' selected='selected'>" + n + "</option>";
	}
	return "<option value='" + v + "'>" + n + "</option>";
}

function _each(data, selectid, selvalue,un) {
	$.each(data, function(k, p) {
		if (p.pcode == selvalue) {
			$(selectid).append(_option(p.name, p.code ,un));
		}
	});
}

function _remove(a) {
	$("#" + a + " option:gt(0)").remove();
